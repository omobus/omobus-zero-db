<?xml version="1.0" encoding="utf-8"?>
<contacts templ="%contact_id%;%account_id%;%name%;%surname%;%patronymic%;%job_title_id%;%mobile%;%email%;%spec_id%;%loyalty_level_id%;%locked%;%extra_info%;">
  <![CDATA[
select contact_id, account_id, name, patronymic, surname, job_title_id, mobile, email, spec_id, loyalty_level_id, locked, extra_info from contacts
