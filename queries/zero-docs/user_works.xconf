<?xml version="1.0" encoding="utf-8"?>
<user_works templ="%user_id%;%w_cookie%;%fix_date%;%b_dt%;%b_la%;%b_lo%;%b_sat_dt%;%e_dt%;%e_la%;%e_lo%;%e_sat_dt%;">
<check_exist>
  <![CDATA[
select count(*) exist from user_works 
    where user_id = '%user_id%' and w_cookie = '%w_cookie%'
  ]]>
</check_exist>
<update>
  <![CDATA[
update user_works set e_dt=dbo.datetime_in('%e_dt%'), e_la=dbo.gps_in('%e_la%'), e_lo=dbo.gps_in('%e_lo%'), e_sat_dt=dbo.datetime_in('%e_sat_dt%')
    where user_id = '%user_id%' and w_cookie = '%w_cookie%'
  ]]>
</update>
<insert>
  <![CDATA[
insert into user_works (user_id, w_cookie, fix_date, b_dt, b_la, b_lo, b_sat_dt, e_dt, e_la, e_lo, e_sat_dt) 
    values ('%user_id%', '%w_cookie%', '%fix_date%', '%b_dt%', dbo.gps_in('%b_la%'), dbo.gps_in('%b_lo%'), dbo.datetime_in('%b_sat_dt%'), dbo.datetime_in('%e_dt%'), dbo.gps_in('%e_la%'), dbo.gps_in('%e_lo%'), dbo.datetime_in('%e_sat_dt%'))
  ]]>
</insert>
</user_works>