<?xml version="1.0" encoding="utf-8"?>
<unsched templ="%doc_id%;%fix_dt%;%user_id%;%unsched_type_id%;%doc_note%;">
<check_exist>
  <![CDATA[
select count(*) exist from unsched 
    where doc_id = '%doc_id%'
  ]]>
</check_exist>
<insert>
  <![CDATA[
insert into unsched (doc_id, fix_dt, user_id, unsched_type_id, doc_note) 
    values ('%doc_id%', '%fix_dt%', '%user_id%', '%unsched_type_id%', '%doc_note%')
  ]]>
</insert>
</unsched>