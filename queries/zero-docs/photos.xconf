<?xml version="1.0" encoding="utf-8"?>
<photos templ="%doc_id%;%fix_dt%;%user_id%;%account_id%;%placement_id%;%brand_id%;%photo_type_id%;%photo%;%doc_note%;%photo_param_ids%;">
<check_exist>
  <![CDATA[
select count(*) exist from photos 
    where doc_id = '%doc_id%'
  ]]>
</check_exist>
<insert>
  <![CDATA[
insert into photos (doc_id, fix_dt, user_id, account_id, placement_id, brand_id, photo_type_id, photo, doc_note, photo_param_ids) 
    values ('%doc_id%', '%fix_dt%', '%user_id%', '%account_id%', '%placement_id%', '%brand_id%', '%photo_type_id%', dbo.uid_in('%photo%'), dbo.note_in('%doc_note%'), dbo.uids_in('%photo_param_ids%'))
  ]]>
</insert>
</photos>